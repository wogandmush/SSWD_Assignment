<?php
include 'header.php';
if(!$isAdmin){
	header("Location: $root/index.php");
	exit();
}

if($_SERVER['REQUEST_METHOD'] !== 'POST'){
	echo "<h4 class='text-success'>Great Success</h4>";
	$createNew = new Button("action", "create-new");
	$actionForm = new Form('action-form', 'feature_manage.php', 'POST');
	$actionForm->addButtons($createNew);
	$actionForm->render();
}
else{
	if(!isset($_POST['action'])){
		header("refresh: 2");
		exit();
	}
	

	

}
